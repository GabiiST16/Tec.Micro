	RCALL M2
	LDI R17, 0X00
	OUT PORTD, R17
	RET
AB:
	LDI R17, 0X00
	LDI R17, 0X14
	OUT PORTD, R17
	RET
DER:
	LDI R17, 0X00
	LDI R17, 0X44
	OUT PORTD, R17
	RET
IZQ:
	LDI R17, 0X00
	LDI R17, 0X84
	OUT PORTD, R17
	RET
AR:
	LDI R17, 0X00
	LDI R17, 0X24
	OUT PORTD, R17
	RET