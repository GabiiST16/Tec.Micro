.include "m328pdef.inc"
.org 0x00
    RJMP start

start: 
;Inicio stack pointer
    ldi r16, low(ramend)
    out spl, r16
    ldi r16, high(ramend)
    out sph, r16
;Defino puertos de salida
    ldi r16, 0x7F
    out DDRD, r16; puerto D para M1, M2 y M3
    ldi r16, 0x04
    out DDRB, r16
;Defino puertos de entrada
